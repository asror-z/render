    function reformatIcons(){
        
     $("[class='glyphicon glyphicon-trash']").attr('class', 'fas fa-trash');
    $("[class='glyphicon glyphicon-zoom-in']").attr('class', 'fas fa-search-plus');
    $("[class='glyphicon glyphicon-move']").attr('class', 'fas fa-arrows-alt');
